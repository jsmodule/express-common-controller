{
  "name": "express-common-controller",
  "version": "1.0.2",
  "description": "Common base controller implemented by express",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "node public/server.js",
    "lint": "eslint src",
    "clean": "rm -rf ./lib ./public",
    "build": "npm run clean && babel src -d lib && babel examples -d public",
    "test": "npm run lint && mocha --compilers js:babel-register test/js/",
    "prepublish": "npm run clean && npm run build",
    "release": "/bin/bash ./ci/git-release.sh"
  },
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "express-controller-middleware": "^0.4.0",
    "mocha": "^2.5.3",
    "path": "^0.12.7",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmodule/express-common-controller.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "controller"
  ],
  "author": "thomascxq <thomascxq@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsmodule/express-common-controller/issues"
  },
  "homepage": "https://github.com/jsmodule/express-common-controller#readme"
}
